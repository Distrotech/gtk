msgstr "Дали тази икона за панела има подсказка"

#: ../gtk/gtkstatusicon.c:365 ../gtk/gtkwidget.c:1074
msgid "Tooltip Text"
msgstr "Текстът на подсказка"

#: ../gtk/gtkstatusicon.c:366 ../gtk/gtkwidget.c:1075 ../gtk/gtkwidget.c:1096
